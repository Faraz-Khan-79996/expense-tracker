{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      },
      {
        "src": "build/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/api",
        "dest": "index.js"
      },
      {
        "handle": "filesystem"
      },
      {
        "src": "/api/(.*)",
        "dest": "index.js"
      },
      {
        "src": "/",
        "dest": "build/index.html"
      },
      { "src": "/[^.]+", "build": "/", "status": 200 },
      {
        "src": "/(.+)",
        "dest": "build/$1"
      }
    ]
  }